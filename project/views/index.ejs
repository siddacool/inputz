<!DOCTYPE html>
<html>

<head>
    <title><%= title %> - Components</title>
    <% include ./partials/headSection %>
    <link rel='stylesheet' href='./css/main.css' />
    <link rel='stylesheet' href='./css/dos.css' />
    <% include ./partials/googleFonts %>
</head>

<body>
   <% include ./partials/svgIcons %>
    <div id="wrapper">
        <header>
            <div class="constrainBox">
                <h1 id="title"><a href="/"><%= title %></a></h1>
                <p id="description" >
                    Details
                </p>
            </div>
        </header>
        <main>
           <div class="constrainBox">
               <div class="componentsHolder">


                  <% include ./inputzComponents/textfields %>
                  <% include ./inputzComponents/buttons %>
