                       <p style="text-align:center"><a href="https://inputz.herokuapp.com/"class="normal">Download</a></p>
               </div>
           </div>
          
        </main>
    </div>


    <script type="text/javascript" src="js/script.js"></script>
     <script type="text/javascript" src="js/componentsUi.js"></script>
     <script type="text/javascript" src="js/inputz.js"></script>

    <script>
      izObject.checkbox();
      izObject.radio();
      izObject.toggle();
